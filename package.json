  "dependencies": {
    "web-worker": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "mocha": "^10.6.0",
    "rollup": "^4.23.0",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "should": "^13.2.3"
